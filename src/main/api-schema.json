                "examples": {
                  "Success Response": {
                    "value": "{\n  \"statusCode\": 200,\n  \"body\": {\n      \"description\": \"Any description 2\",\n      \"name\": \"Robert\",\n      \"dob\": \"1995-12-17T03:24:00\",\n      \"address\": \"Z Street, 9120\",\n      \"id\": 3,\n      \"createdAt\": \"2021-06-01T00:06:57.593Z\",\n      \"updatedAt\": \"2021-06-01T00:06:57.593Z\"\n  }\n}"
